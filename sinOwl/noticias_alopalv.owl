<owl:NamedIndividual rdf:about="#https://elpais.com/sociedad/2025-03-19/espana-pionera-en-hacer-la-ropa-realmente-mas-sostenible.html">
  <rdf:type rdf:resource="#NewsItem"/>
  <creator rdf:resource="#C.V."/>
  <mentions rdf:resource="#Galicia"/>
  <mentions rdf:resource="#Medio_ambiente"/>
  <mentions rdf:resource="#Unión_Europea"/>
