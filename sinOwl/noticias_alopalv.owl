  <creator rdf:resource="#Carmen_Torrecillas"/>
  <creator rdf:resource="#Adrián_Maqueda_(Civio)"/>
  <creator rdf:resource="#Ter_García"/>
  <mentions rdf:resource="#Reglamentos"/>
  <mentions rdf:resource="#España"/>
  <mentions rdf:resource="#Algoritmos_computacionales"/>
