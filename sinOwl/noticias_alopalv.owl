<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://iptc.org/std/rNews/2011-10-07"
         xmlns="http://iptc.org/std/rNews/2011-10-07#">

<owl:Ontology rdf:about="#">
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The IPTC rNews Ontology (draft 3, @agebhard, June 9, 2012)</rdfs:comment>
</owl:Ontology>

<owl:ObjectProperty rdf:about="#about">
  <rdfs:range rdf:resource="#Concept"/>
  <rdfs:domain rdf:resource="#NewsItem"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Indicates that the NewsItem is specifically about a concept.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#accountablePerson">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="#Person"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Specifies the person who is legally accountable for the NewsItem.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#address">
  <rdfs:range rdf:resource="#PostalAddress"/>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Organization"/>
        <rdf:Description rdf:about="#Person"/>
        <rdf:Description rdf:about="#Place"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A real-world postal address associated with this entity.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#associatedArticle">
  <rdfs:range rdf:resource="#Article"/>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#AudioObject"/>
        <rdf:Description rdf:about="#ImageObject"/>
        <rdf:Description rdf:about="#VideoObject"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">An Article associated with the Media Object.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#associatedMedia">
  <rdfs:domain rdf:resource="#Article"/>
  <owl:inverseOf rdf:resource="#associatedArticle"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#AudioObject"/>
        <rdf:Description rdf:about="#ImageObject"/>
        <rdf:Description rdf:about="#VideoObject"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">An  image or audio or video object associated with a NewsItem.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#comment">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="#UserComment"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Comments on this NewsItem, typically from readers/users.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#contributor">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Organization"/>
        <rdf:Description rdf:about="#Person"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A secondary contributor to the NewsItem, not the primary creator.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#copyrightHolder">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Organization"/>
        <rdf:Description rdf:about="#Person"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The entity holding the legal copyright to the NewsItem.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#creator">
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Organization"/>
        <rdf:Description rdf:about="#Person"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#NewsItem"/>
        <rdf:Description rdf:about="#UserComment"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The author, videographer, photographer etc. (creator) of the NewsItem or Comment.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#discusses">
  <rdfs:range rdf:resource="#NewsItem"/>
  <rdfs:domain rdf:resource="#UserComment"/>
  <owl:inverseOf rdf:resource="#comment"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Specifies the NewsItem associated with this Comment.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#editor">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Organization"/>
        <rdf:Description rdf:about="#Person"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Specifies the person or organization who edited the NewsItem.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#geoCoordinates">
  <rdfs:range rdf:resource="#GeoCoordinates"/>
  <rdfs:domain rdf:resource="#Place"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The geo coordinates of the location.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#mentions">
  <rdfs:range rdf:resource="#Concept"/>
  <rdfs:domain rdf:resource="#NewsItem"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Indicates that the NewsItem contains a reference to, but is not necessarily about a concept.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#provider">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Organization"/>
        <rdf:Description rdf:about="#Person"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Specifies the person or organization that distributed the NewsItem.  </rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#sourceOrganization">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="#Organization"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The organization on whose behalf the creator was working.</rdfs:comment>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#additionalName">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The middle name portion of a full name for a person.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#addressCountry">
  <rdfs:domain rdf:resource="#PostalAddress"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A country, part of the address. Implementors are encouraged (but not required) to use ISO 3166-1 alpha-2 country codes.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#addressLocality">
  <rdfs:domain rdf:resource="#PostalAddress"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A city/town/village etc. part of the address.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#addressRegion">
  <rdfs:domain rdf:resource="#PostalAddress"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A subdivision of a country, part of the address.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#alternativeHeadline">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A secondary headline or title of a NewsItem.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#articleBody">
  <rdfs:domain rdf:resource="#Article"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The main text of the article.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#articleSection">
  <rdfs:domain rdf:resource="#Article"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The top-level classification of the content by the publishing organization.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#box">
  <rdfs:domain rdf:resource="#GeoCoordinates"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A box is the area enclosed by the rectangle formed by two points. The first point is the lower corner, the second point is the upper corner. A box is expressed as two points separated by a space character.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#circle">
  <rdfs:domain rdf:resource="#GeoCoordinates"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A circle is the circular region of a specified radius centered at a specified latitude and longitude. A circle is expressed as a pair followed by a radius in meters.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#commentText">
  <rdfs:domain rdf:resource="#UserComment"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The text of the comment.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#commentTime">
  <rdfs:domain rdf:resource="#UserComment"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The date and time at which the comment was left.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#copyrightNotice">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Any necessary copyright notice for claiming the intellectual property for the NewsItem.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#copyrightYear">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The year during which the claimed copyright was first asserted.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#dateCreated">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The date (and optionally time) on which the NewsItem was created.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#dateModified">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The date (and optionally time) on which the NewsItem was most recently modified.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#datePublished">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The date (and optionally time) on which the NewsItem was first made available online.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#dateline">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The location where the NewsItem was produced.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#description">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A free-form textual description of the NewsItem. Publishers may find it helpful to view this as the "caption" for a Media object.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#discussionUrl">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A link to the page containing the comments relating to this NewsItem.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#duration">
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#AudioObject"/>
        <rdf:Description rdf:about="#VideoObject"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#duration"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <rdfs:comment xml:lang="en">The duration of the audio or video object in ISO 8601 date format. (see also http://en.wikipedia.org/wiki/ISO_8601)</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#elevation">
  <rdfs:domain rdf:resource="#GeoCoordinates"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The elevation is the height above sea level in meters of a specified point. An Elevation attribute must be specified in conjunction with a Point attribute.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#email">
  <rdfs:domain rdf:resource="#PostalAddress"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">An email address.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#encodingFormat">
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#AudioObject"/>
        <rdf:Description rdf:about="#ImageObject"/>
        <rdf:Description rdf:about="#VideoObject"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The encoding of a media asset. The encoding must be drawn from the IPTC NewsCodes for Images, Audio codecs or Video codecs available at http://cv.iptc.org/newscodes/format/, http://cv.iptc.org/newscodes/audiocodec/, and http://cv.iptc.org/newscodes/videocodec/ respectively.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#familyName">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Family name. In the U.S., this is the last name of an individual.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#faxNumber">
  <rdfs:domain rdf:resource="#PostalAddress"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">An international fax number.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#featureCode">
  <rdfs:domain rdf:resource="#Place"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A code indicating the type of the geographic entity specified by a location object. Implementors are encouraged (but not required) to populate this field with values drawn from the controlled vocabulary of feature codes provided by geonames.org.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#genre">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:unionOf>
    </rdfs:Datatype>
  </rdfs:range>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">A term describing the general editorial category of the article. Implementors are encouraged (but not required) to draw values for type of material from the IPTC Genre subject headings at http://cv.iptc.org/newscodes/genre/.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#givenName">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">Given name. In the U.S., this is the first name of an individual. Also used if the individual has only one name.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#headline">
  <rdfs:domain rdf:resource="#NewsItem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.0</owl:versionInfo>
  <rdfs:comment xml:lang="en">The main (or only) headline or title for a NewsItem</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#height">
  <rdfs:range>
    <rdfs:Datatype>
      <owl:unionOf rdf:parseType="Collection">
